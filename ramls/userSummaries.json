{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Collection of user summaries",
  "properties": {
    "userSummaries": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "userSummary.json"
      }
    },
    "totalRecords": {
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "required": [
    "userSummaries",
    "totalRecords"
  ]
}
